    "total_price = int(input())\n",
    "item_count = int(input())\n",
    "spc = 0\n",
    "for i in range(item_count):\n",
    "    price, count = map(int, input().split())\n",
    "    pc = price * count\n",
    "    spc = spc + pc\n",
    "\n",
    "if spc == total_price)"
